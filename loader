-- Compact loader for GitHub raw usage
-- Usage from executor: loadstring(game:HttpGet("RAW_URL"))()
local payload_b64 = [===[

]===]

local http = game:GetService("HttpService")
local ok, code = pcall(http.Base64Decode, http, payload_b64)
if not ok then error("Base64 decode failed") end
local fn, err = loadstring(code)
if not fn then error(err) end
fn()
